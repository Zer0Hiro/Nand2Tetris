/**
 * Computes the difference of three bits.
 */
CHIP FullSub {
    IN a,b,bIn;
    OUT diff, // Difference between a-b-bIn
        bOut; // Borrowed part

    PARTS:
    HalfSub(a=a, b=b, diff=adifb, bOut=bOut1);
    HalfSub(a=adifb, b=bIn, diff=diff, bOut=bOut2);
    Or(a=bOut1, b=bOut2, out=bOut);


}
